<h1>Account Settings</h1>
<%= form_for @user do |f| %>
	<%= f.label :first_name, 'First' %><br/>
	<%= f.text_field :first_name %><br/><br/>
	<%= f.label :last_name, 'Last' %><br/>
	<%= f.text_field :last_name %><br/><br/>
	<%= f.label :nickname %><br/>
	<%= f.text_field :nickname %><br/><br/>
	<%= f.label :email %><br/>
	<%= f.text_field :email %><br/>
	<br/>
	<%= f.submit %>
<% end %>
